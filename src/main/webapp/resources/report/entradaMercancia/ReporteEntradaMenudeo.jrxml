		<field name="TOTAL" class="java.math.BigDecimal"/>
		<variable name="Variable_1" class="java.math.BigDecimal" calculation="Sum">
			<variableExpression><![CDATA[$F{PRECIO}]]></variableExpression>
		</variable>
